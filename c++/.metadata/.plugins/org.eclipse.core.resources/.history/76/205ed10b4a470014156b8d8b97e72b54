#include "Nodo.h"

namespace itla {

Nodo::Nodo() {
	// TODO Auto-generated constructor stub

}

Nodo::~Nodo()
{
	Numero=0;
	Enlace=NULL;
	// TODO Auto-generated destructor stub
}

} /* namespace itla */
void itla::Nodo::Set_numero(int N) {

	Numero=N;
}

void itla::Nodo::Set_enlace(Nodo* E) {
	Enlace=E;
}

int itla::Nodo::Get_numero() {
	return Numero;
}

Nodo* itla::Nodo::Get_enlace() {

	return Enlace;

}
